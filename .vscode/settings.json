{
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.format.enable": true,
  "eslint.alwaysShowStatus": true,
  "editor.detectIndentation": false,
  "typescript.validate.enable": true,
  "editor.formatOnSave": true,
  "laravel-pint.enable": true,
  "laravel-pint.enableDebugLogs": true,
  "laravel-pint.configPath": "server/pint.json",
  "[php]": {
    "editor.defaultFormatter": "open-southeners.laravel-pint"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "jestrunner.jestCommand": "yarn run test:api:separately",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#8553b3",
    "activityBar.background": "#6153b3",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#bc12cd",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#1153b3",
    "statusBar.background": "#055488",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#1153b3",
    "statusBarItem.remoteBackground": "#059488",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#024488",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#05248899",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#0d9488",
  "cSpell.words": ["laravel"]
}
